{
  "name": "mbaynton/csv4twig",
  "description": "A CSV escaping filter for the Twig templating engine.",
  "minimum-stability": "stable",
  "license": "MIT",
  "authors": [
    {
      "name": "Mike Baynton",
      "email": "mike@mbaynton.com"
    }
  ],
  "require": {
    "php": ">=5.4.0"
  },
  "require-dev": {
    "phpunit/phpunit": "~4.8"
  },
  "autoload": {
    "psr-4": {
      "mbaynton\\CSV4Twig\\": "src/"
    }
  },
  "autoload-dev": {
    "psr-4": {
      "mbaynton\\CSV4Twig\\Tests\\": "tests/src/"
    }
  },
  "suggest": {
    "twig/twig": "While there are no hard dependencies on Twig, it's kind of the point."
  },
  "keywords": ["Templating"],
  "config": {
    "platform": {
      "php": "5.4.0"
    }
  }
}
